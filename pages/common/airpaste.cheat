% airpaste, common

# Wait for a message and display it when received
airpaste

# Send text
echo <text> | airpaste

# Send a file
airpaste < <path_to_file>

# Receive a file
airpaste > <path_to_file>

# Create or join a channel
airpaste <channel_name>
