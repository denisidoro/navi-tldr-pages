% androguard, common

# Display Android app manifest
androguard axml <path_to_app.apk>

# Display app metadata (version and app ID)
androguard apkid <path_to_app.apk>

# Decompile Java code from an app
androguard decompile <path_to_app.apk> --output <path_to_directory>
