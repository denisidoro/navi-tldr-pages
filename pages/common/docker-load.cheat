% docker-load, common

# Load a Docker image from `stdin`
docker load < <path_to_image_file.tar>

# Load a Docker image from a specific file
docker load --input <path_to_image_file.tar>

# Load a Docker image from a specific file in quiet mode
docker load --quiet --input <path_to_image_file.tar>
