% pueue-completions, common

# Generate completions for Bash
sudo pueue completions bash <usr_share_bash-completion_completions_pueue.bash>

# Generate completions for zsh
sudo pueue completions zsh <usr_share_zsh_site-functions>

# Generate completions for fish
sudo pueue completions fish <usr_share_fish_completions>
